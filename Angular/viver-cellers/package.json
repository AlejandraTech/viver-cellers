{
  "name": "viver-cellers",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run watch:css && ng serve",
    "build": "npm run build:css && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:css": "tailwindcss -i ./src/styles.css -o ./src/assets/styles/main.css",
    "watch:css": "tailwindcss -i ./src/styles.css -o ./src/assets/styles/main.css --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@stripe/stripe-js": "^3.4.0",
    "flowbite": "^2.3.0",
    "rxjs": "~7.8.0",
    "stripe-angular": "^1.9.3",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.10",
    "@angular/cli": "^16.2.10",
    "@angular/compiler-cli": "^16.2.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.10",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.1.3"
  }
}
